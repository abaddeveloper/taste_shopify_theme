				'accountID': {{ section.settings.account_id }},
				'SKU': "{{ section.settings.sku}}",
				'features': "{{ section.settings.initial_features }}",
				'country': "{{ section.settings.country }}",
				'viewerType': "{{ section.settings.viewer_type }}",  
